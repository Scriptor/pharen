(check (map (lambda (n)
                    (* n 2)) [1 2 3]) [2 4 6])


(fn apply-test (f n)
  ($f n))
(check (apply-test (lambda (n)
                           (. n " bar")) "foo") "foo bar")

(fn greet-generator-test (s)
  "line1"
  (lambda () (. "Hello " s "!")))
(check ((greet-generator-test "Hammurabi")) "Hello Hammurabi!")

(fn multiple-calls-test ()
  (lambda () "foobar"))
(multiple-calls-test)
(multiple-calls-test)
(check ((multiple-calls-test)) "foobar")
